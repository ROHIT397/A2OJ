link - https://codeforces.com/problemset/problem/451/B

#include<bits/stdc++.h>
using namespace std;
#define pb push_back
#define mp make_pair
typedef long long int ll;

int main()
{
    int n;
    cin >> n;
    int a[n];
    int b[n];
    for (int i = 0; i < n; ++i)
    {
        cin >> a[i];
        b[i] = a[i];
    }
    sort(b, b + n);
    int count = 0;
    for (int i = 0; i < n; i++)
    {
        if (a[i] == b[i])
            count++;
    }
    if (count == n)
    {
 
        cout << "yes\n1 1\n";
        return 0;
    }
    int j = 0, k = 0, l = 0;
    for (int i = 0; i < n - 1, j < 1;)
    {
        if (a[i] < a[i + 1])
        {
            i++;
        }
        else
        {
            k = i; l = i;
            while (a[i] > a[i + 1] && i < n - 1)
            {
                i++;
                l++;
            }
            sort(a + k, a + l + 1);
            j++;
        }
    }
    for (int i = 0; i < n; i++)
    {
        if (a[i] != b[i])
        {
            cout << "no\n";
            return 0;
        }
    }
    cout << "yes\n" << k + 1 << " " << l + 1 << "\n";
 
}
